{
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "$id": "https://raw.githubusercontent.com/educredentials/obv3-examples/refs/heads/main/schemas/extracurruricular.json",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "achievement": {
          "type": "object",
          "properties": {
            "ECTS": {
              "type": "number",
              "minimum": 0.5,
              "maximum": 240,
              "multipleOf": 0.5
            }
          },
          "required": [
            "ECTS"
          ]
        }
      }
    }
  }
}
