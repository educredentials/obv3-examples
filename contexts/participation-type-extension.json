{
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "title": "Participation Form",
  "description": "An extension allowing for the addition of information regarding the type of participation that has taken place to achieve this achievement.",
  "@context": {
    "ParticipationType": {
      "@id": "https://example.org/ParticipationType",
      "@type": "@vocab",
      "@context": {
        "online": "https://example.org/ParticipationType/online",
        "onsite or blended": "https://example.org/ParticipationType/onsite-or-blended",
        "volunteering": "https://example.org/ParticipationType/volunteering",
        "work experience": "https://example.org/ParticipationType/work-experience",
        "personalized learning activities": "https://example.org/ParticipationType/personalized-learning-activities"
      }
    }
  }
}
